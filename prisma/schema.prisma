generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Trip {
  id String @id @default(uuid())

  destination String
  startsAt    DateTime
  endsAt      DateTime
  isConfirmed Boolean  @default(false)

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("trips")
}
